<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <!-- Idle -->
  <AnimationDef>
    <defName>OneSin_Idle</defName>
    <durationTicks>120</durationTicks>
    <playWhenDowned>true</playWhenDowned>
    <animationParts>
      <!-- Root the all -->
      <li>
        <key>Root</key>
        <value>
          <workerClass>AnimationWorker_Keyframes</workerClass>
          <keyframes>
            <li>
              <tick>0</tick>
              <offset>(0,0,0)</offset>
            </li>
            <li>
              <tick>60</tick>
              <offset>(0,0,-0.1)</offset>
            </li>
            <li>
              <tick>120</tick>
              <offset>(0,0,0)</offset>
            </li>
          </keyframes>
        </value>
      </li>
    </animationParts>
  </AnimationDef>

</Defs>